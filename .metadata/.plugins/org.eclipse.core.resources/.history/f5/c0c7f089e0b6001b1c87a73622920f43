package design.duck;

public class DuckSimulation {

	public static void main(String[] args) {
		Duck myDuck = new WoodDuck();
		myDuck.display();
		myDuck.performFly();
		myDuck.swimming();
		//myDuck.performQuack();
		
		Duck yourDuck = new RubberDuck();
		yourDuck.display();
		yourDuck.performFly();
		yourDuck.swimming();
		//yourDuck.performQuack();
		
		Duck ourDuck = new MalladDuck();
		ourDuck.display();
		ourDuck.performFly();
		ourDuck.swimming();
		//ourDuck.performQuack();
	}

}
